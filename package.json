{
  "name": "ser-responsavel",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:prettier:check": "prettier . --check",
    "lint:prettier:write": "prettier . --write",
    "postinstall": "nuxt prepare",
    "format": "prettier . --write",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run",
    "supabase:init": "pnpx supabase init",
    "supabase:login": "pnpx supabase login",
    "supabase:link": "pnpx supabase link --project-ref mqcsfvmzqegblqjkvisp",
    "supabase:types": "pnpx supabase gen types typescript --project-id mqcsfvmzqegblqjkvisp --schema public > database/types.ts",
    "db:migrate:new": "pnpx supabase migration new",
    "db:reset": "pnpx supabase db reset --linked",
    "db:seed": "node --env-file=.env database/seed.js",
    "prepare": "husky"
  },
  "lint-staged": {
    "**/*.{js,ts,vue}": [
      "prettier --write",
      "eslint --fix"
    ],
    "**/*.{json,md,css,scss,html,yaml,yml}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "@nuxt/eslint": "1.9.0",
    "@nuxt/fonts": "0.11.4",
    "@nuxt/icon": "2.0.0",
    "@nuxt/image": "1.11.0",
    "@nuxtjs/supabase": "1.6.1",
    "@pinia/nuxt": "^0.11.2",
    "@supabase/supabase-js": "^2.57.4",
    "@vueuse/core": "^13.9.0",
    "@vueuse/nuxt": "^13.9.0",
    "eslint": "^9.35.0",
    "nuxt": "^4.1.2",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.5.0",
    "sass-embedded": "^1.92.1",
    "vue": "^3.5.21",
    "vue-router": "^4.5.1",
    "vuetify": "^3.10.1",
    "vuetify-nuxt-module": "0.18.7"
  },
  "pnpm": {
    "overrides": {
      "esbuild": ">=0.25.0"
    }
  },
  "packageManager": "pnpm@10.13.1+sha512.37ebf1a5c7a30d5fabe0c5df44ee8da4c965ca0c5af3dbab28c3a1681b70a256218d05c81c9c0dcf767ef6b8551eb5b960042b9ed4300c59242336377e01cfad",
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@faker-js/faker": "^10.0.0",
    "@nuxt/test-utils": "^3.19.2",
    "@vitest/ui": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "esbuild": "^0.25.9",
    "eslint-config-prettier": "^10.1.8",
    "happy-dom": "^18.0.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
